<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="zicht_versioning.command.testutil" class="Zicht\Bundle\VersioningBundle\Command\TestUtilCommand">
            <tag name="console.command"/>
        </service>
        <service id="zicht_versioning.command.admin" class="Zicht\Bundle\VersioningBundle\Command\AdminCommand">
            <tag name="console.command"/>
            <argument type="service" id="zicht_versioning.manager" />
            <argument type="service" id="doctrine" />
        </service>
        <service id="zicht_versioning.command.validate" class="Zicht\Bundle\VersioningBundle\Command\ValidateCommand">
            <tag name="console.command"/>
            <argument type="service" id="zicht_versioning.manager" />
            <argument type="service" id="doctrine" />
        </service>
    </services>
</container>
